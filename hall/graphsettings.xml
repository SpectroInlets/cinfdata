<?xml version='1.0' standalone='yes'?>
<graphs>
  <!-- TEMPERATURE -->
  <graph type='temperature'>
    <matplotlib_settings>
      <width>1400</width>
      <height>800</height>
    </matplotlib_settings>

    <right_legend_suffix> (r)</right_legend_suffix>
    <default_xscale>dat</default_xscale>
    <title>Status for experimental hall</title>
    <ylabel>Temperature / C</ylabel>
    <dateplot1>
      <query>SELECT unix_timestamp(time), value FROM dateplots_hall
      where type = 166 and value between -1 and 1300 and time between
      "{from}" and "{to}" order by time</query>
      <ylabel>Temperature / C</ylabel>
      <title>Temperature in the experimental hall</title>
      <legend>T. hall</legend>
      <color>1</color>
    </dateplot1>
    <dateplot2>
      <query>SELECT unix_timestamp(time), value FROM dateplots_hall
      where type=160 and value > -1 and time between "{from}" and
      "{to}" order by time</query>
      <ylabel>Pressure / Bar</ylabel>
      <title>Cooling water pressure</title>
      <legend>Pressure</legend>
      <color>2</color>
    </dateplot2>
    <dateplot3>
      <query>SELECT unix_timestamp(time), value FROM dateplots_hall
      where type=161 and time between "{from}" and "{to}" order by
      time</query>
      <ylabel>Pressure / Bar</ylabel>
      <title>N5 argon pressure</title>
      <legend>Pressure</legend>
      <color>3</color>
    </dateplot3>
    <dateplot4>
      <query>SELECT unix_timestamp(time), value FROM dateplots_hall
      where type=162 and time between "{from}" and "{to}" order by
      time</query>
      <ylabel>Temperature / C</ylabel>
      <title>Cooling water return temperature</title>
      <legend>Temperature</legend>
      <color>4</color>
    </dateplot4>
    <dateplot5>
      <query>SELECT unix_timestamp(time), value FROM dateplots_hall
      where type=163 and time between "{from}" and "{to}" order by
      time</query>
      <ylabel>Temperature / C</ylabel>
      <title>Cooling water forward temperature</title>
      <legend>Temperature</legend>
      <color>5</color>
    </dateplot5>
    <dateplot7>
      <query>SELECT unix_timestamp(time), value FROM dateplots_hall
      where type = 167 and time between "{from}" and "{to}" order by
      time</query>
      <ylabel>Pressure / Pa</ylabel>
      <title>Ventilation pressure</title>
      <legend>P. ventilation</legend>
      <color>7</color>
    </dateplot7>
    <dateplot8>
      <query>SELECT unix_timestamp(time), value FROM dateplots_hall
      where type = 168 and time between "{from}" and "{to}" order by
      time</query>
      <ylabel>Humidity / %</ylabel>
      <title>Humidity</title>
      <legend>Humidity</legend>
      <color>8</color>
    </dateplot8>
  </graph>

  <!-- TEMPERATURE -->
  <graph type='work'>
    <matplotlib_settings>
      <width>1400</width>
      <height>800</height>
    </matplotlib_settings>

    <right_legend_suffix> (r)</right_legend_suffix>
    <default_xscale>dat</default_xscale>
    <title>Workday Burndown</title>
    <ylabel>Temperature / C</ylabel>
    <dateplot1>
      <query>select unix_timestamp(time), value from dateplots_hall where type=(SELECT id FROM dateplots_descriptions where codename="work_completed") and time between "{from}" and "{to}" order by time</query>
      <ylabel>Hours of work</ylabel>
      <title>Work completed</title>
      <legend>Completed</legend>
      <color>1</color>
    </dateplot1>
    <dateplot2>
      <query>select unix_timestamp(time), value from dateplots_hall where type=(SELECT id FROM dateplots_descriptions where codename="work_estimate") and time between "{from}" and "{to}" order by time</query>
      <ylabel>Hours of work</ylabel>
      <title>Estimate</title>
      <legend>Estimate</legend>
      <color>2</color>
    </dateplot2>
    <dateplot3>
      <query>select unix_timestamp(time), value from dateplots_hall where type=(SELECT id FROM dateplots_descriptions where codename="work_total") and time between "{from}" and "{to}" order by time</query>
      <ylabel>Hours of work</ylabel>
      <title>Work Total</title>
      <legend>Total</legend>
      <color>3</color>
    </dateplot3>
    <dateplot4>
      <query>select unix_timestamp(time), value from dateplots_hall where type=(SELECT id FROM dateplots_descriptions where codename="work_left_minus_started") and time between "{from}" and "{to}" order by time</query>
      <ylabel>Hours of work</ylabel>
      <ylabel>Hours of work</ylabel>
      <title>Work left - started</title>
      <legend>Left - started</legend>
      <color>4</color>
    </dateplot4>
    <dateplot5>
      <query>select unix_timestamp(time), value from dateplots_hall where type=(SELECT id FROM dateplots_descriptions where codename="work_left") and time between "{from}" and "{to}" order by time</query>
      <ylabel>Hours of work</ylabel>
      <title>Work left</title>
      <legend>Left</legend>
      <color>5</color>
    </dateplot5>
  </graph>

