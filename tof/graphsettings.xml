    <label_column>id</label_column>
    <image_format>png</image_format>
    <mandatory_export_fields>
      <field0>
        <field>id</field>
        <name>Id</name>
      </field0>
      <field1>
        <field>type</field>
        <name>Type</name>
      </field1>
      <field2>
        <field>time</field>
        <name>Recorded at</name>
      </field2>
      <field3>
        <field>comment</field>
        <name>Comment</name>
      </field3>
    </mandatory_export_fields>
  </global_settings>

</graphs>
